    <ui:InvokeCode ContinueOnError="{x:Null}" Code="' Create Excel Application object&#xD;&#xA;Dim excelApp As New Microsoft.Office.Interop.Excel.Application()&#xD;&#xA;&#xD;&#xA;Try&#xD;&#xA;    ' Open Excel file&#xD;&#xA;    Dim workbook As Microsoft.Office.Interop.Excel.Workbook = excelApp.Workbooks.Open(target_excel_path)&#xD;&#xA;&#xD;&#xA;    ' Avoid pop-up windows&#xD;&#xA;    excelApp.DisplayAlerts = False&#xD;&#xA;&#xD;&#xA;    ' Get worksheet&#xD;&#xA;    Dim worksheet As Microsoft.Office.Interop.Excel._Worksheet = CType(workbook.Sheets(target_excel_sheet_name), Microsoft.Office.Interop.Excel._Worksheet)&#xD;&#xA;&#xD;&#xA;    ' Get cell&#xD;&#xA;    Dim cell As Microsoft.Office.Interop.Excel.Range = CType(worksheet.Range(target_excel_cell), Microsoft.Office.Interop.Excel.Range)&#xD;&#xA;&#xD;&#xA;    ' Insert object&#xD;&#xA;    Dim oleObjects As Microsoft.Office.Interop.Excel.OLEObjects = CType(worksheet.OLEObjects(Type.Missing), Microsoft.Office.Interop.Excel.OLEObjects)&#xD;&#xA;    oleObjects.Add(Filename:=insert_file_path, DisplayAsIcon:=True, IconFileName:=insert_file_icon,IconIndex:=0,IconLabel:=insert_file_description, Left:=cell.Left, Top:=cell.Top)&#xD;&#xA;&#xD;&#xA;    'System.Threading.Thread.Sleep(5000)&#xD;&#xA;    'excelApp.AlertBeforeOverwriting = False&#xD;&#xA;    ' Save and close workbook&#xD;&#xA;    workbook.Save()&#xD;&#xA;    workbook.Close()&#xD;&#xA;&#xD;&#xA;Catch ex As Exception&#xD;&#xA;    ' Handle error&#xD;&#xA;    error_message = ex.Message&#xD;&#xA;&#xD;&#xA;Finally&#xD;&#xA;    ' Release resources and close Excel application&#xD;&#xA;    excelApp.Quit()&#xD;&#xA;    System.Runtime.InteropServices.Marshal.ReleaseComObject(excelApp)&#xD;&#xA;End Try&#xD;&#xA;" DisplayName="Invoke Code" sap:VirtualizedContainerService.HintSize="423.2,164.8" sap2010:WorkflowViewState.IdRef="InvokeCode_1">
